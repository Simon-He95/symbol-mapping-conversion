{
  "publisher": "simonhe",
  "name": "symbol-mapping-conversion",
  "displayName": "symbol-mapping-conversion",
  "version": "0.0.16",
  "packageManager": "pnpm@10.20.0",
  "description": "symbol-mapping-conversion",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/symbol-mapping-conversion#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/symbol-mapping-conversion"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/symbol-mapping-conversion/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.101.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "symbol-mapping-conversion",
      "properties": {
        "symbol-mapping-conversion.mappings": {
          "type": "object",
          "default": {},
          "description": "%symbol-mapping-conversion.mappings%"
        },
        "symbol-mapping-conversion.isEnable": {
          "type": "boolean",
          "default": true,
          "description": "%symbol-mapping-conversion.isEnable%"
        },
        "symbol-mapping-conversion.extLanguage": {
          "type": "array",
          "default": [],
          "description": "%symbol-mapping-conversion.extLanguage%"
        },
        "symbol-mapping-conversion.copyMap": {
          "type": "boolean",
          "default": false,
          "description": "%symbol-mapping-conversion.copyMap%"
        },
        "symbol-mapping-conversion.pairMappings": {
          "type": "object",
          "default": {
            "{": "}",
            "[": "]",
            "(": ")",
            "<": ">",
            "'": "'",
            "\"": "\"",
            "`": "`"
          },
          "description": "%symbol-mapping-conversion.pairMappings%"
        },
        "symbol-mapping-conversion.bulkDetection": {
          "type": "object",
          "default": {
            "maxChanges": 20,
            "maxCharsPerChange": 1000,
            "maxLinesPerChange": 20,
            "maxRecentSize": 2000,
            "minFullReplaceLines": 50,
            "highFrequencyInterval": 50,
            "highFrequencyChanges": 10
          },
          "description": "%symbol-mapping-conversion.bulkDetection%",
          "properties": {
            "maxChanges": {
              "type": "number",
              "default": 20,
              "description": "%symbol-mapping-conversion.bulkDetection.maxChanges%"
            },
            "maxCharsPerChange": {
              "type": "number",
              "default": 1000,
              "description": "%symbol-mapping-conversion.bulkDetection.maxCharsPerChange%"
            },
            "maxLinesPerChange": {
              "type": "number",
              "default": 20,
              "description": "%symbol-mapping-conversion.bulkDetection.maxLinesPerChange%"
            },
            "maxRecentSize": {
              "type": "number",
              "default": 2000,
              "description": "%symbol-mapping-conversion.bulkDetection.maxRecentSize%"
            },
            "minFullReplaceLines": {
              "type": "number",
              "default": 50,
              "description": "%symbol-mapping-conversion.bulkDetection.minFullReplaceLines%"
            },
            "highFrequencyInterval": {
              "type": "number",
              "default": 50,
              "description": "%symbol-mapping-conversion.bulkDetection.highFrequencyInterval%"
            },
            "highFrequencyChanges": {
              "type": "number",
              "default": 10,
              "description": "%symbol-mapping-conversion.bulkDetection.highFrequencyChanges%"
            }
          }
        },
        "symbol-mapping-conversion.bulkDetectionOverrides": {
          "type": "object",
          "default": {},
          "description": "%symbol-mapping-conversion.bulkDetectionOverrides%"
        },
        "symbol-mapping-conversion.autoPause": {
          "type": "object",
          "default": {
            "enabled": false,
            "triggerCount": 10,
            "durationMs": 60000
          },
          "description": "%symbol-mapping-conversion.autoPause%",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": false,
              "description": "%symbol-mapping-conversion.autoPause.enabled%"
            },
            "triggerCount": {
              "type": "number",
              "default": 10,
              "description": "%symbol-mapping-conversion.autoPause.triggerCount%"
            },
            "durationMs": {
              "type": "number",
              "default": 60000,
              "description": "%symbol-mapping-conversion.autoPause.durationMs%"
            }
          }
        },
        "symbol-mapping-conversion.autoActionOnSkip": {
          "type": "object",
          "default": {
            "enabled": false,
            "snippet": "",
            "languageFilter": []
          },
          "description": "%symbol-mapping-conversion.autoActionOnSkip%",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": false,
              "description": "%symbol-mapping-conversion.autoActionOnSkip.enabled%"
            },
            "snippet": {
              "type": "string",
              "default": "",
              "description": "%symbol-mapping-conversion.autoActionOnSkip.snippet%"
            },
            "languageFilter": {
              "type": "array",
              "default": [],
              "description": "%symbol-mapping-conversion.autoActionOnSkip.languageFilter%"
            },
            "includeReasonVariable": {
              "type": "boolean",
              "default": true,
              "description": "%symbol-mapping-conversion.autoActionOnSkip.includeReasonVariable%"
            }
          }
        },
        "symbol-mapping-conversion.notifications": {
          "type": "object",
          "default": {
            "showSkipToasts": false,
            "detailedTooltip": true
          },
          "description": "%symbol-mapping-conversion.notifications%",
          "properties": {
            "showSkipToasts": {
              "type": "boolean",
              "default": false,
              "description": "%symbol-mapping-conversion.notifications.showSkipToasts%"
            },
            "detailedTooltip": {
              "type": "boolean",
              "default": true,
              "description": "%symbol-mapping-conversion.notifications.detailedTooltip%"
            },
            "tooltipDurationMs": {
              "type": "number",
              "default": 4000,
              "description": "%symbol-mapping-conversion.notifications.tooltipDurationMs%"
            }
          }
        },
        "symbol-mapping-conversion.autoRemediation": {
          "type": "object",
          "default": {
            "enabled": false,
            "triggerCount": 5,
            "snippet": "",
            "pauseAfterRun": false,
            "snippetKey": "",
            "languageFilter": []
          },
          "description": "%symbol-mapping-conversion.autoRemediation%",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": false,
              "description": "%symbol-mapping-conversion.autoRemediation.enabled%"
            },
            "triggerCount": {
              "type": "number",
              "default": 5,
              "description": "%symbol-mapping-conversion.autoRemediation.triggerCount%"
            },
            "snippet": {
              "type": "string",
              "default": "",
              "description": "%symbol-mapping-conversion.autoRemediation.snippet%"
            },
            "pauseAfterRun": {
              "type": "boolean",
              "default": false,
              "description": "%symbol-mapping-conversion.autoRemediation.pauseAfterRun%"
            },
            "snippetKey": {
              "type": "string",
              "default": "",
              "description": "%symbol-mapping-conversion.autoRemediation.snippetKey%"
            },
            "languageFilter": {
              "type": "array",
              "default": [],
              "description": "%symbol-mapping-conversion.autoRemediation.languageFilter%"
            }
          }
        }
      }
    },
    "commands": [
      {
        "command": "symbol-mapping-conversion.toggleStatusBar",
        "title": "%symbol-mapping-conversion.toggleStatusBar%"
      },
      {
        "command": "symbol-mapping-conversion.showMenu",
        "title": "%symbol-mapping-conversion.showMenu%"
      },
      {
        "command": "symbol-mapping-conversion.showLog",
        "title": "%symbol-mapping-conversion.showLog%"
      },
      {
        "command": "symbol-mapping-conversion.createMappingFromSelection",
        "title": "%symbol-mapping-conversion.createMappingFromSelection%"
      },
      {
        "command": "symbol-mapping-conversion.showInsights",
        "title": "%symbol-mapping-conversion.showInsights%"
      },
      {
        "command": "symbol-mapping-conversion.clearBulkStats",
        "title": "%symbol-mapping-conversion.clearBulkStats%"
      },
      {
        "command": "symbol-mapping-conversion.pauseBulkDetection",
        "title": "%symbol-mapping-conversion.pauseBulkDetection%"
      },
      {
        "command": "symbol-mapping-conversion.manageBulkOverrides",
        "title": "%symbol-mapping-conversion.manageBulkOverrides%"
      },
      {
        "command": "symbol-mapping-conversion.showSkipHistory",
        "title": "%symbol-mapping-conversion.showSkipHistory%"
      }
    ]
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.3.0",
    "@types/node": "^22.16.0",
    "@types/vscode": "^1.101.0",
    "@vscode-use/utils": "^0.1.59",
    "@vscode/vsce": "^3.6.0",
    "bumpp": "^9.11.1",
    "eslint": "^9.30.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
